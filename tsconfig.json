{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["ES2018", "DOM"],
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": ".",
  },
  "typedocOptions": {
    "out": "docs",
    "entryPoints": [
      "./src/index.ts",
    ],
    "name": "TypeDocBugRepro",
    "excludeExternals": true,
    "excludePrivate": true,
  }
}
